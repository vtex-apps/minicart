{
  "vendor": "vtex",
  "name": "minicart",
  "title": "Mini Cart",
  "version": "2.1.0",
  "defaultLocale": "pt-BR",
  "builders": {
    "pages": "0.x",
    "react": "2.x"
  },
  "mustUpdateAt": "2019-04-20",
  "categories": [],
  "registries": [
    "smartcheckout"
  ],
  "settingsSchema": {},
  "scripts": {
    "postreleasy": "vtex publish --verbose"
  },
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/checkout/pub/*"
      }
    }
  ],
  "dependencies": {
    "vtex.store": "1.x",
    "vtex.store-components": "2.x",
    "vtex.styleguide": "6.x",
    "vtex.use-svg": "0.x"
  }
}
