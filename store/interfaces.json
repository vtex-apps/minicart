{
  "minicart": {
    "allowed": ["sandbox", "product-summary"],
    "component": "index"
  },
  "minicart.v2": {
    "component": "Minicart",
    "composition": "children",
    "allowed": "*"
  },
  "minicart-content": {
    "component": "Content",
    "composition": "children",
    "allowed": "*"
  },
  "minicart-base-content": {
    "component": "BaseContent",
    "allowed": [
      "minicart-product-list",
      "minicart-summary",
      "minicart-empty-state",
      "minicart-content"
    ],
    "render": "lazy"
  },
  "minicart-product-list": {
    "component": "ProductList",
    "allowed": ["product-list"],
    "render": "lazy"
  },
  "minicart-summary": {
    "component": "Summary",
    "allowed": ["checkout-summary"],
    "render": "lazy"
  },
  "minicart-empty-state": {
    "component": "EmptyState",
    "composition": "children",
    "allowed": "*",
    "render": "lazy"
  }
}
